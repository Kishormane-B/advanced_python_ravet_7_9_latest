    "# Decorators\n",
    "To modify the behaviour of function"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 10,
   "id": "491af31b",
   "metadata": {},
   "outputs": [],
   "source": [
    "def welcome(func):\n",
    "    def wrapper(*args, **kwargs):\n",
    "        print(\"Welcome User!\")\n",
    "        res = func(*args, **kwargs)\n",
    "        print(f\"function Results : {res}\")\n",
    "        print(\"Thank you\")\n",
    "        return res\n",
    "\n",
    "    return wrapper"
   ]
